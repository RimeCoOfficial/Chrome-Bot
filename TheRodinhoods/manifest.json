{
  "name": "Chrome-Bot TheRodinhoods",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A auto tasking extension for Google Chrome.",
  // "homepage_url": "http://",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "src/options/index.html",
  "background": {
    "scripts": ["src/inject/inject.js"]
  },
  "permissions": [
    "http://therodinhoods.com/*",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}