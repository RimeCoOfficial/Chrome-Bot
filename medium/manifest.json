{
  "name": "Chrome-Bot Medium",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A auto tasking extension for Google Chrome.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["src/inject.js"]
  },
  "permissions": [
    "https://medium.com/*",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}